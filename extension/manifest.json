{
  "manifest_version": 3,
  "name": "Course Extractor",
  "version": "1.1",
  "description": "Extracts course data from MyCamu and sends it to the timetable generator.",

  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage"
  ],

  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_title": "Course Extractor"
  }

 
}
